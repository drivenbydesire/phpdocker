{
    "name": "portal",
    "description": "Any Application Tool to Monitor Performance and Tail Logs",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">8.1.0",
        "ext-soap": "*"

    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "codeception/codeception": "^2.3",
        "larapack/dd": "1.*",
        "filp/whoops": "^2.12",
        "ext-json": "*",
        "squizlabs/php_codesniffer": "3.*",
        "phpcompatibility/phpcompatibility": "^9.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "scripts": {
        "test": "php vendor/bin/codecept run unit"
    },
    "authors": [
        {
            "name": "Sanjeev Sarkar",
            "email": "formallysanjeev@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": false
        }
    }
}